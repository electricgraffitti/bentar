<td bgcolor="#FFFFFF" valign="top" width="400" style="font-size:12px;color:#000000;line-height:150%;font-family:trebuchet ms;">
  <p>
    <span style="font-size:20px;font-weight:bold;color:#444444;font-family:arial;line-height:110%;">Bentar.com :: Contact Request</span><br>
     <ul>
       <li>From: <%= @associate[:name] %></li>
       <li>Business: <%= @associate[:business_name] %></li>
       <li>Email: <%= @associate[:email] %></li>
       <li>Phone: <%= @associate[:phone] %></li>
       <li>Street: <%= @associate[:street] %></li>
       <li>City: <%= @associate[:city] %></li>
       <li>State: <%= @associate[:state] %></li>
       <li>Zip Code: <%= @associate[:zipcode] %></li>
     </ul>
     <br>
     <br>
     <span style="font-size:15px;font-weight:bold;color:#444444;font-family:arial;line-height:110%;">Comment:</span><br>
     <%= @associate[:message] %>
     <br>
     <br>
     <span style="font-size:11px;font-weight:normal;color:#666666;font-style:italic;font-family:arial;">Generated at <%= @sent_on.to_s %></span><br>
  </p>
</td>